{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "bracketSpacing": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "requirePaddingNewLinesBetweenLines": true,
  "importOrder": ["^react$", "^@mui", "^@(components|assets|image|utils|services)(/.*)?$", "^[./]"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
